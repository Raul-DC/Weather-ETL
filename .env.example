# API key for accessing the weather data API
API_KEY=your_api_key_here

# Path to the service account JSON file for Google BigQuery authentication
CREDENTIALS_PATH=/path/to/your/service_account.json

# PostgreSQL connection string
POSTGRES_URL=postgresql://username:password@localhost:port/dbname

# BigQuery Table ID (format: project_id.dataset_id.table_id)
TABLE_ID=your_project_id.your_dataset_id.your_table_id
